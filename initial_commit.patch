+
+        super(m);
+        this.value = value;
+    }
+
+    public Storage(Mnemonic m, byte[] symbol) {
+
+        super(m);
+        this.data = symbol;
+    }
+
+    @Override
+    public int length() {
+
+        if (this.data == null) {
+
